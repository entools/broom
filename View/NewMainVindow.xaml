<Window x:Class="Revit.UserControl2"             
        x:Name="wins"
             
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:Revit"
        Icon="../settings.ico"
        mc:Ignorable="d" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Height="400" Width="400" Title="ENTOOLS BROOM">

    <Window.Resources>
        <ResourceDictionary Source="../Properties/StyleButton.xaml"></ResourceDictionary>
    </Window.Resources>


    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="120"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Button Style="{StaticResource GetButton}"
                    Content="Delete parameters"/>
            <Button Style="{StaticResource GetButton}"
                    Content="Purge unused"/>
            <Button Style="{StaticResource GetButton}"
                    Content="Delete worksets"/>
            <Button Style="{StaticResource GetButton}"
                    Content="Delete view, legend, sheets"/>
            <!--ETC-->


        </StackPanel>
    </Grid>

</Window>


